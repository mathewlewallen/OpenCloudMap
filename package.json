    "@types/ms": "^0.7.34",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "postcss": "^8.5.3",
    "react-email": "3.0.6",
    "tailwindcss": "^4.1.4",
    "typescript": "^5",
    "wrangler": "^4.4.0"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=22"
  }
}
