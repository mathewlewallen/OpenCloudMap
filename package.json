{
  "name": "opencloudmap",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "check": "opennextjs-cloudflare build && tsc && wrangler deploy --dry-run",
    "deploy": "opennextjs-cloudflare build && wrangler deploy",
    "preview": "opennextjs-cloudflare build && wrangler dev",
    "build:analyze": "ANALYZE=true next build",
    "db:generate": "drizzle-kit generate --name",
    "db:migrate:dev": "wrangler d1 migrations apply $(node scripts/get-db-name.mjs) --local",
    "d1:cache:clean": "wrangler d1 execute $(node scripts/get-db-name.mjs) --command \"DELETE FROM tags; DELETE FROM revalidations\" --remote",
    "d1:cache:seed": "wrangler d1 execute $(node scripts/get-db-name.mjs) --file .open-next/cloudflare/cache-assets-manifest.sql --remote",
    "email:dev": "email dev -d src/react-email -p 3001"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@icons-pack/react-simple-icons": "^12.7.0",
    "@marsidev/react-turnstile": "^1.1.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-accordion": "^1.2.8",
    "@radix-ui/react-avatar": "^1.1.7",
    "@radix-ui/react-collapsible": "^1.1.8",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@react-email/components": "0.0.36",
    "@react-email/render": "1.0.6",
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.1",
    "@stripe/react-stripe-js": "^3.6.0",
    "@stripe/stripe-js": "^7.2.0",
    "@tanstack/react-table": "^8.21.3",
    "arctic": "^3.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "framer-motion": "12.9.2",
    "ipaddr.js": "^2.2.0",
    "lucide-react": "^0.503.0",
    "motion": "^12.9.2",
    "ms": "^2.1.3",
    "next": "15.3.1",
    "next-themes": "^0.4.6",
    "nextjs-toploader": "^3.8.16",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.1",
    "react-turnstile": "^1.1.4",
    "remeda": "^2.21.3",
    "sonner": "^2.0.3",
    "stripe": "^18.0.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "ua-parser-js": "^2.0.3",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.24.3",
    "zsa": "^0.6.0",
    "zsa-react": "^0.2.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.1",
    "@opennextjs/cloudflare": "1.0.0-beta.4",
    "@opennextjs/aws": "3.5.8",
    "@simplewebauthn/types": "^12.0.0",
    "@tailwindcss/postcss": "4.1.4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "drizzle-kit": "^0.31.0",
    "postcss": "^8.5.3",
    "react-email": "4.0.7",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "wrangler": "^4.14.0"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=22"
  }
}
